<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="dlgSelectAttributes"
        noDataText="No Attributes Found"
        title="{i18n>Create_Attribute_Group_SelectAttributes}"
        search=".onSearchAttributes"
        confirm=".onDialogClose"
        cancel=".onDialogClose"
        items="{
        path: 'appModel>/AssociatedAttributes',
        filters: [{
            path: 'Rank',
            operator: 'EQ',
            value1: 0
        }]
    }"
        multiSelect="true"
        contentHeight="70%"
        contentWidth="10%"
    >
        <StandardListItem title="{appModel>name}" />
    </SelectDialog>
</core:FragmentDefinition>
